                        </div>
                        <div class="form-group col-md-8">
                            <label class="control-label">Mobile</label>
                            @Html.DropDownListFor(vm => vm.poste_Souhaite.Mobile, new SelectList(new List<Object> { new { value = 0, text = "Oui" }, new { value = 1, text = "Non" } }, "value", "text"), "Veuillez Selectionner", new { @class = "form-validate form-control col", required = "true" })
                        </div>
                        <button class="btn btn-outline prevBtn" type="button" style="float:left">Précédent </button>
                        <button class="btn btn-success" type="submit" style="float:right">Enregistrer</button>
                    </div>
